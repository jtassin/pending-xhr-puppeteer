{
  "name": "pending-xhr-puppeteer",
  "node": ">= 8.0.0",
  "version": "2.0.0",
  "description": "Small tool for wait that all xhr are finished in pupeteer",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/es.js",
  "homepage": "https://github.com/jtassin/pending-xhr-puppeteer",
  "repository": {
    "type": "git",
    "url": "https://github.com/jtassin/pending-xhr-puppeteer.git"
  },
  "bugs": {
    "url": "https://github.com/jtassin/pending-xhr-puppeteer/issues"
  },
  "author": "Julien TASSIN",
  "license": "MIT",
  "private": false,
  "keywords": [
    "puppeteer",
    "xhr",
    "pending",
    "wait",
    "nodejs",
    "e2e"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/puppeteer": "^1.12.3",
    "auto-changelog": "^1.12.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "3.6.0",
    "eslint-plugin-jest": "22.4.1",
    "husky": "1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "8.1.5",
    "prettier": "1.17.0",
    "puppeteer": "1.11.0",
    "release-it": "^10.3.1",
    "rollup": "1.0.0",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-plugin-typescript2": "^0.20.1",
    "ts-jest": "^24.0.0",
    "tsc": "^1.20150623.0",
    "tslib": "^1.9.3",
    "typescript": "^3.3.4000"
  },
  "scripts": {
    "prettier:write": "prettier --single-quote --trailing-comma es5 --write src/**/*",
    "lint": "eslint src",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "precommit": "lint-staged",
    "build": "rollup -c",
    "publish:patch": "yarn publish --patch"
  },
  "release-it": {
    "scripts": {
      "changelog": "npx auto-changelog --stdout --commit-limit false -u",
      "beforeStage": "npx auto-changelog"
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
